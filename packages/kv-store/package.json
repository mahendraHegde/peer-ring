    "distributed",
    "decentralized",
    "sharding",
    "dynamo",
    "peer to peer cache",
    "distributed-systems",
    "high availability",
    "consistent hashing",
    "distributed applications",
    "peer discovery",
    "load balancing",
    "replication",
    "distributed quorum",
    "dynamic allocation"
  ],
  "author": "Mahendra Hegde",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
